<h1>New Project</h1>

<div class = "row">
	<div class = "span6 offset3">

<%= form_for(@project) do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.submit "Save changes", class: "btn btn-large btn-primary"%>

<% end %>
	
	</div>
</div>
